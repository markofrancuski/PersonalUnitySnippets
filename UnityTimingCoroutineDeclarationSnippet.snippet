    <Header>
      <Title>Timing Coroutine </Title>
      <Shortcut>cor</Shortcut>
      <Description>Returns Delcartion private Timing Coroutine</Description>
      <Author>Marko Francuski</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
        <SnippetType>SurroundsWith</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>

      <Code Language="csharp">
        <![CDATA[private IEnumerator<float> _ExampleCoroutine()
        {
        yield return Timing.WaitForOneFrame;
        }]]>
      </Code>
    </Snippet>
  </CodeSnippet>
